{
  "name": "react-table-components",
  "version": "0.0.12",
  "description": "React components for rendering table data",
  "author": "Vladimir Kolebidenko",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/votsa/react-table-components"
  },
  "keywords": [
    "react",
    "table"
  ],
  "main": "./lib/index",
  "files": [
    "styles",
    "lib",
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm run lint && npm run test && npm run build:js",
    "build:js": "webpack --config webpack.config.prod.js && babel src -d lib",
    "start": "webpack-dev-server -d",
    "lint": "eslint src --ext js,jsx --fix; exit 0",
    "test": "npm run lint && jest"
  },
  "jest": {
    "roots": [
      "src"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.3.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "css-loader": "^0.28.9",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "filemanager-webpack-plugin": "^1.0.24",
    "jest": "^19.0.2",
    "lodash-webpack-plugin": "^0.11.2",
    "react-test-renderer": "^16.2.0",
    "style-loader": "^0.16.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
